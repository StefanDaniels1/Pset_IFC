<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../PSD_R2x3.xsl"?>
<PropertySetDef>
    <IfcVersion version="2x3 TC1"/>
    <Name>Pset_ConcreteElementGeneral</Name>
    <Definition>Definition from IAI: General properties common to different types of concrete elements. The Pset can be
        used by a number of subtypes of IfcBuildingElement.
    </Definition>
    <Applicability>General pset</Applicability>
    <ApplicableClasses>
        <ClassName>IfcBeam</ClassName>
        <ClassName>IfcBuildingElementPart</ClassName>
        <ClassName>IfcBuildingElementProxy</ClassName>
        <ClassName>IfcColumn</ClassName>
        <ClassName>IfcCovering</ClassName>
        <ClassName>IfcCurtainWall</ClassName>
        <ClassName>IfcDoor</ClassName>
        <ClassName>IfcFooting</ClassName>
        <ClassName>IfcMember</ClassName>
        <ClassName>IfcPile</ClassName>
        <ClassName>IfcRailing</ClassName>
        <ClassName>IfcRamp</ClassName>
        <ClassName>IfcRampFlight</ClassName>
        <ClassName>IfcRoof</ClassName>
        <ClassName>IfcSlab</ClassName>
        <ClassName>IfcStair</ClassName>
        <ClassName>IfcStairFlight</ClassName>
        <ClassName>IfcWall</ClassName>
        <ClassName>IfcWallStandardCase
        </ClassName>
    </ApplicableClasses>
    <ApplicableTypeValue></ApplicableTypeValue>
    <PropertyDefs>
        <PropertyDef>
            <Name>StructuralClass</Name>
            <PropertyType>
                <TypePropertySingleValue>
                    <DataType type="IfcLabel"/>
                </TypePropertySingleValue>
            </PropertyType>
            <ValueDef/>
            <Definition>The structural class defined for the concrete structure (e.g. '1').</Definition>
        </PropertyDef>
        <PropertyDef>
            <Name>EnvironmentalClass</Name>
            <PropertyType>
                <TypePropertySingleValue>
                    <DataType type="IfcLabel"/>
                </TypePropertySingleValue>
            </PropertyType>
            <ValueDef/>
            <Definition>The environmental class for the concrete structure (e.g. 'Y1')</Definition>
        </PropertyDef>
        <PropertyDef>
            <Name>FireRating</Name>
            <PropertyType>
                <TypePropertySingleValue>
                    <DataType type="IfcLabel"/>
                </TypePropertySingleValue>
            </PropertyType>
            <ValueDef/>
            <Definition>Fire rating given according to the national fire safety classification</Definition>
        </PropertyDef>
        <PropertyDef>
            <Name>ServiceLife</Name>
            <PropertyType>
                <TypePropertySingleValue>
                    <DataType type="IfcTimeMeasure"/>
                    <UnitType type="TIMEUNIT"/>
                </TypePropertySingleValue>
            </PropertyType>
            <ValueDef/>
            <Definition>Expected service life (usually measured in years)</Definition>
        </PropertyDef>
        <PropertyDef>
            <Name>LifeCycleEnvironmentalLoad</Name>
            <PropertyType>
                <TypePropertySingleValue>
                    <DataType type="IfcText"/>
                </TypePropertySingleValue>
            </PropertyType>
            <ValueDef/>
            <Definition>Description of the life-cycle environmental load</Definition>
        </PropertyDef>
        <PropertyDef>
            <Name>DimensionalAccuracyClass</Name>
            <PropertyType>
                <TypePropertySingleValue>
                    <DataType type="IfcLabel"/>
                </TypePropertySingleValue>
            </PropertyType>
            <ValueDef/>
            <Definition>Classification designation of the dimensional accuracy requirement according to local
                standards.
            </Definition>
        </PropertyDef>
        <PropertyDef>
            <Name>ConstructionToleranceClass</Name>
            <PropertyType>
                <TypePropertySingleValue>
                    <DataType type="IfcLabel"/>
                </TypePropertySingleValue>
            </PropertyType>
            <ValueDef/>
            <Definition>Classification designation of the on-site construction tolerances according to local
                standards.
            </Definition>
        </PropertyDef>
        <PropertyDef>
            <Name>ConstructionType</Name>
            <PropertyType>
                <TypePropertySingleValue>
                    <DataType type="IfcLabel"/>
                </TypePropertySingleValue>
            </PropertyType>
            <ValueDef/>
            <Definition>Designator for whether the concrete element is constructed on site or prefabricated. Allowed
                values are: 'Insitu' vs 'Precast'
            </Definition>
        </PropertyDef>
        <PropertyDef>
            <Name>ConcreteCoverAtMainBars</Name>
            <PropertyType>
                <TypePropertySingleValue>
                    <DataType type="IfcPositiveLengthMeasure"/>
                    <UnitType type="LENGTHUNIT"/>
                </TypePropertySingleValue>
            </PropertyType>
            <ValueDef/>
            <Definition>The protective concrete cover at the main reinforcing bars according to local building
                regulations.
            </Definition>
        </PropertyDef>
        <PropertyDef>
            <Name>ConcreteCoverAtLinks</Name>
            <PropertyType>
                <TypePropertySingleValue>
                    <DataType type="IfcPositiveLengthMeasure"/>
                    <UnitType type="LENGTHUNIT"/>
                </TypePropertySingleValue>
            </PropertyType>
            <ValueDef/>
            <Definition>The protective concrete cover at the reinforcement links according to local building
                regulations.
            </Definition>
        </PropertyDef>
    </PropertyDefs>
</PropertySetDef>
